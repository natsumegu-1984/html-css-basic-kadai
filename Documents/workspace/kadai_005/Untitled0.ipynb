        "# 台形の上辺と下辺、高さを定義\n",
        "upmost = 10\n",
        "down = 20\n",
        "high = 5\n",
        "\n",
        "# 台形の面積を計算\n",
        "area = (upmost + down) * high / 2\n",
        "\n",
        "# 面積の表示\n",
        "print(str(area) + \"cm²\")"
